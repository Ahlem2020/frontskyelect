  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:fast": "set FAST_REFRESH=true && next dev --turbo",
    "dev:instant": "set NODE_OPTIONS=--max-old-space-size=4096 && set FAST_REFRESH=true && next dev --turbo",
